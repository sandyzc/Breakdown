<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sandyzfeaklab.breakdown_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Breakdown">
        <activity android:name="com.sandyzfeaklab.breakdown_app.Add_Sap_codes"></activity>
        <activity android:name="com.sandyzfeaklab.breakdown_app.Mttr" />
        <activity
            android:name="com.sandyzfeaklab.breakdown_app.Data_input"
            android:label="@string/title_activity_data_input"
            android:theme="@style/Theme.Breakdown.NoActionBar" />
        <activity
            android:name="com.sandyzfeaklab.breakdown_app.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Breakdown.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        android:networkSecurityConfig="@xml/network_security_config"
    </application>

</manifest>